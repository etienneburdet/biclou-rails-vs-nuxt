<div class="cointainer">
  <p>Vous voulez un vélo à <%= @place %></p>
</div>

<ul>
  <% @bikes.each do |bike| %>
    <%= link_to new_bike_rental_path(bike) do %>
      <li><%= bike.name %></li>
    <% end %>
  <% end %>
</ul>

<div
  id="map"
  style="width: 100%;
  height: 600px;"
  data-markers="<%= @markers.to_json %>"
  data-mapbox-api-key="<%= ENV['MAPBOX_API_KEY'] %>"
></div>
